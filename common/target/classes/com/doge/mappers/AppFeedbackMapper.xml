			</if>
		</trim>
			on DUPLICATE key update
		<trim prefix="" suffix="" suffixOverrides=",">
			<if test= "bean.nickName!=null"> 
				nick_name = VALUES(nick_name),
			</if>
			<if test= "bean.content!=null"> 
