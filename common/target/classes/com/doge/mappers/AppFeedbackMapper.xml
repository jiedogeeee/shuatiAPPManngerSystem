			</if>
		</trim>
		</insert>
	<!--批量插入-->
		<insert id ="insertBatch" parameterType="com.doge.entity.po.AppFeedback">
			INSERT INTO app_feedback(user_id,nick_name,content,create_time,p_feedback_id,status,send_type,client_last_send_time)values
			<foreach collection="list" item="item" separator=",">
			(#{item.userId},#{item.nickName},#{item.content},#{item.createTime},#{item.pFeedbackId},#{item.status},#{item.sendType},#{item.clientLastSendTime})
			</foreach>
		</insert>

